{
  "name": "simple-axios-wechat",
  "version": "0.1.2",
  "description": "小于1kb的简易的axios微信小程序版使用typescript实现",
  "keywords": [
    "axios",
    "wechat",
    "http",
    "request",
    "weixin",
    "ajax"
  ],
  "author": "Jaye",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lei1248276/simple-axios-wechat"
  },
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/simple-axios-wechat.js",
  "module": "./dist/simple-axios-wechat.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/simple-axios-wechat.js",
      "require": "./dist/simple-axios-wechat.cjs"
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.4",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "miniprogram-api-typings": "^3.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2"
  }
}
